package com.jcore.repository;
import java.util.List;
public interface JpaRepository<T> {
	void devuelve_error() throws Exception;//dispara exception si el registro no es encontrado
	Integer insert(T t) throws Exception;
	Integer update(T t) throws Exception;
	Integer delete(T t) throws Exception;
	List<T> devuelveTodos() throws Exception;
	T buscaPorLLave(T t) throws Exception;//busca por llave primaria y devuelve una unica fila
}
